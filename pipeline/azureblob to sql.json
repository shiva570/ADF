{
	"name": "azureblob to sql",
	"properties": {
		"activities": [
			{
				"name": "Cars csv to sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"preferredRegions": []
					},
					"sink": {
						"type": "JsonSink",
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "setOfObjects"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['Id']"
								},
								"sink": {
									"path": "$['Id']"
								}
							},
							{
								"source": {
									"path": "$['_rid']"
								},
								"sink": {
									"path": "$['_rid']"
								}
							},
							{
								"source": {
									"path": "$['_self']"
								},
								"sink": {
									"path": "$['_self']"
								}
							},
							{
								"source": {
									"path": "$['_etag']"
								},
								"sink": {
									"path": "$['_etag']"
								}
							},
							{
								"source": {
									"path": "$['_attachments']"
								},
								"sink": {
									"path": "$['_attachments']"
								}
							},
							{
								"source": {
									"path": "$['_ts']"
								},
								"sink": {
									"path": "$['_ts']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "InputCosmosCars",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "outputJson",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}